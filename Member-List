aura_env.checkBlacklist = function(name)
    for _, player in pairs(aura_env.blacklist) do
        if name == player  then
            return true
        end
    end
    return false
end

aura_env.displayPopupWindow = function(name)
    StaticPopupDialogs["BLACKLIST"] = {
        text = "[WARNING] ".. name .. " is blacklisted on Benediction. It is recomended that you do not play with this player.",
        button1 = "Okay",
        timeout = 0,
        whileDead = true,
        hideOnEscape = true,
        preferredIndex = 3
    }
    StaticPopup_Show ("BLACKLIST")
    return
end


